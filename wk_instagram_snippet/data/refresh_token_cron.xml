<odoo>
    <record id="website_insta_refresh_token_cron" model="ir.cron">
        <field name="name">Refresh Token: Instagram Access Token</field>
        <field name="model_id" ref="model_website"/>
        <field name="state">code</field>
        <field name="code">model._cron_refresh_token()</field>
        <field name="interval_number">50</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="active" eval="True"/>
        <field name="doall" eval="False"/>
    </record>
</odoo>
